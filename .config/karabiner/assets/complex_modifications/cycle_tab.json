{
  "title": "Cycle Tabs",
  "rules": [
    {
      "description": "Next Tab: opt+cmd+l",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": ["option", "command"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": ["control"]
            }
          ]
        }
      ]
    },
    {
      "description": "Prev Tab: opt+cmd+h",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": ["option", "command"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": ["control", "shift"]
            }
          ]
        }
      ]
    }
  ]
}
